import java.util.*;
public class magic number 
{
public static void main(String[] args)
{
Scanner obj=new Scanner(System.in);
int num=obj.nextInt();
int n=num;
while(n>9)
{
int sum=0;
while(n>0)
{
int d= n%10;
n=n/10;
sum=sum+d;
}
n=sum;
}
if(n==1)
System.out.println("YES");
else
System.out.println("NO");
}
}
